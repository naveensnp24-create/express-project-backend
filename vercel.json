{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "MONGO_URL": "@mongo_url",
    "SECRET_KEY": "@secret_key",
    "JWT_EXPRESS_IN": "@jwt_express_in",
    "PORT": "3000"
  }
}